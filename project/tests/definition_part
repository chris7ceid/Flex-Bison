}

bool funcWithFor2(int sss)
{
	for(;;)
	{
		;
		break;
	}
	
	for(true; true; true)
	{
		;
		continue rafael;
	}
	
	for(; ; );
	for(true; ; );
	for(true; true; );
	for(true; true; true);
	for(; true; true);
	for(; ; true);
	for(; true; );
	return true;
}

